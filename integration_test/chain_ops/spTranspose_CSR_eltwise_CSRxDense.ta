def main() {
	#IndexLabel Declarations
	IndexLabel [i] = [?];
	IndexLabel [j] = [?];         

	#Tensor Declarations
	Tensor<double> A([i, j], CSR);	  
	Tensor<double> B([j, i], CSR);
    Tensor<double> C([j, i], Dense);
    Tensor<double> D([j, i], Dense);

    #Tensor Readfile Operation      
    A[i, j] = read_from_file(0);
    C[j, i] = 2.3;
    D[j, i] = 0.0;

	#Tensor Transpose
	B[j, i] = transpose(A[i, j],{j,i});
    D[j, i] = B[j, i] .* C[j, i];
	print(D);
}

