def main() {
    #IndexLabel Declarations
    IndexLabel [i, c, n] = [2];
    IndexLabel [m, a, d] = [2];

    Tensor<double> A([c, d, m, n], {Dense});
    Tensor<double> B([i, n, a, d], {Dense});
    Tensor<double> C([m, c], {Dense});
    Tensor<double> D([a, i], {Dense});

    A[c, d, m, n] = 2.3;
    B[i, n, a, d] = 3.4;
    C[m, c] = 1.1;
    D[a, i] = 0.0;

    #Tensor contraction
    D[a, i] = A[c, d, m, n] * B[i, n, a, d] * C[m, c];
    print(D);

}

